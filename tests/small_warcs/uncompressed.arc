<script language="javascript">	
	
	function initJS() 
	{
		MJS.SKR.createOverlay({TAG_ID: 'main_div', OVERLAY_OPTIONS: {fixedcenter:true, visible:false, width:'566px'}, OPEN_ON_CLICK_IDS: ['rate_show_1', 'rate_show_2'] , CLOSE_ON_CLICK_IDS: ['main_div_close_popup', 'main_div_close_other']});
			
		// Call rating overlay in case it is directly called with valid params...
		}
	
	// load this JS function on load...
	YAHOO.util.Event.addListener(window, "load", initJS);

	function sendReq()
	{
		// Parameters to send in JSON format...
		var ob = {					
					folderid	: 9660976,
					radio1		: get_radio_value(document.forms[0].radio1),
					radio2		: get_radio_value(document.forms[0].radio2),
					radio3		: get_radio_value(document.forms[0].radio3),
					comments	: document.getElementById('comments').value
				 };

		var postData = 	{
							func: 'rating_function',
							req_src: 'ajax',
							field: 'params',
							value:YAHOO.lang.JSON.stringify(ob)
						};

		var postDataStr = MJS.AJAX.createPostStr(postData);
		var suc;
		var sUrl = 'http://jobs.monsterindia.com/rating.html';
		callback    =   {
						success: function(o)
								 {
									 if(o.responseText != undefined)
									 {
										var response = YAHOO.lang.JSON.parse(o.responseText);									
										
										// On Success...
										if(response.FLAG == 1)
										{
											MJS.HTML.hideTag('errMsgOnDHTML');

											// Div to show success message...
											document.getElementById('rate_job').innerHTML = '';
											document.getElementById('rate_job').innerHTML = response.ERRMSG;
										}
										else
										{								
											// Div to show error message...
											MJS.SKR.showAlertOnDHTML({ MSG: response.ERRMSG, ID: 'errMsgOnDHTML', SUCCESS: response.FLAG });
										}
									 }
								 },
						failure: function(o)
								 {
									 alert('Request Not Completed');
								 },
						cache: false
					};

		var request = YAHOO.util.Connect.asyncRequest('POST',sUrl,callback,postDataStr);
	}
	
	function get_radio_value(obj)
	{
		var val;
        for (var i=0; i < obj.length; i++)
        {
                if (obj[i].checked)
                {
					val = obj[i].value;
                }
        }

		return val;
	}

	function validation()
	{
		var msg = '';
		var obj;

		if ((!document.forms[0].radio1[0].checked) && (!document.forms[0].radio1[1].checked) && (!document.forms[0].radio1[2].checked) && (!document.forms[0].radio1[3].checked))
		{
			msg = 'Please fill Quality of Job';
			obj = document.forms[0].radio1[0];
		}
		else if ((!document.forms[0].radio2[0].checked) && (!document.forms[0].radio2[1].checked) && (!document.forms[0].radio2[2].checked) && (!document.forms[0].radio2[3].checked))
		{
			msg = 'Please rate the company based on this job';
			obj = document.forms[0].radio2[0];
		}
		else if ((!document.forms[0].radio3[0].checked) && (!document.forms[0].radio3[1].checked) && (!document.forms[0].radio3[2].checked) && (!document.forms[0].radio3[3].checked))
		{
			msg = 'Please fill relevance of this job';
			obj = document.forms[0].radio3[0];
		}

		if(msg)
		{
			MJS.SKR.showAlertOnDHTML({ MSG: msg, ID: 'errMsgOnDHTML', SUCCESS: 0 });
			obj.focus();
		}
		else
		{
			sendReq();
		}
	}
	
	function first_step()
	{	
